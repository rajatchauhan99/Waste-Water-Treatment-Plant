import dagshub
dagshub.init(repo_owner='rajatchauhan99', repo_name='Waste-Water-Treatment-Plant', mlflow=True)

import mlflow
with mlflow.start_run():
  mlflow.log_param('parameter name', 'value')
  mlflow.log_metric('metric name', 1)